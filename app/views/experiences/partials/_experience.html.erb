<script id="experience-detail-template" type="text/x-handlebars-template">
	<div class="experience-header">
		{{#if gallery }}
			<img src="{{ gallery.img_name.url }}" alt="">
		{{/if}}
		<span class="year">{{getYear date}}</span>	
		<h1>{{ title }}</h1>
		<span class="company-name">{{ company_name }}</span>
		<span class="company-web"><a href="http://{{ company_web }}" target="_blank">{{ company_web }}</a></span>
	</div>
	<div class="content">
		{{ country }} - {{ date }}

		{{{ content }}}
	</div>

</script>

<script id="experience-detail-title" type="text/x-handlebars-template">
	<h1><a href="/experiencias"><%= t("Experience") %></a></h1>
</script>




