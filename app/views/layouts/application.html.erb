  <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.min.css">
  <link rel="stylesheet" href="/css/theme.css">
  <link rel="stylesheet" href="/bower_components/fontawesome/css/font-awesome.min.css" />
  <link rel="stylesheet" href="/bower_components/nprogress/nprogress.css">
  <%= csrf_meta_tags %>
</head>
<body id="ppu-app">

  <div class="container app-container">
    <nav class="navbar navbar-default navbar-app hidden-lg" role="navigation">
      <div class="container-fluid">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse-1">
           Menú <i class="fa fa-bars"></i>
         </button>
         <a href="/"><img src="/img/Logo@2x.png" alt="" class="img-responsive"></a>
       </div>

       <!-- Collect the nav links, forms, and other content for toggling -->
       <div class="collapse navbar-collapse" id="navbar-collapse-1">
        <ul class="nav navbar-nav">
          <li><a href="/abogados"><%= t("Lawyers") %></a></li>
          <li><a href="/areas"><%= t("Practice Areas") %></a></li>
          <li><a href="/nosotros"><%= t("Us") %></a></li>
          <li><a href="/experiencias"><%= t("Experience") %></a></li>
          <li><a href="/trabaje-con-nosotros"><%= t("Work with Us") %></a></li>
          <li><a href="/probono">Pro bono</a></li>
        </ul>

      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>
  <div class="row">

    <div id="nav-main-container" class="col-lg-2 visible-lg">
      <ul class="nav-main col-lg-1">
        <li class="link-main">
          <a href="/"><img src="/img/Logo@2x.png" alt="" class="img-responsive"></a>
        </li>
        <li><a href="/abogados"><%= t("Lawyers") %></a></li>
        <li><a href="/areas"><%= t("Practice Areas") %></a></li>
        <li><a href="/nosotros"><%= t("Us") %></a></li>
        <li><a href="/experiencias"><%= t("Experience") %></a></li>
        <li><a href="/trabaje-con-nosotros"><%= t("Work with Us") %></a></li>
        <li><a href="/probono">Pro bono</a></li>
      </ul>
    </div>

    <div class="top-bar-container">
      <div class="container">
        <div class="row">
          <div class="col-lg-2"></div>
          <div class="col-lg-8">
            <div id="top-bar"></div>
          </div>
          <div class="col-lg-2 visible-lg change-lang-container">
             <div id="change-lang">
               <a href="#" class="change-lang-page"><%= t('page_lang') %></a>
             </div>
          </div>
        </div>
      </div>
    </div>

    <div class="col-lg-10 col-md-12" id="yield-container">
      <%= yield %>
    </div>

    <div class="footer-container">
      <div class="container">
        <div class="row">
          <div class="col-lg-2"></div>
          <div class="col-lg-10">
            <div id="footer">
              <div  class="collapse" id="footer-content">
                   <a href="#footer-content" class="open-contact" data-toggle="collapse"><%= t("contact") %> <i class="fa fa-chevron-down"></i></a>
              <a href="#footer-content" class="close-footer" data-toggle="collapse"><i class="fa fa-times"></i></a>
         
              <hr>
              <div class="row" >
         
                 <div class="col-lg-3">
                  <ul class="select-cities">
                <li><a data-toggle="collapse" data-parent="#footer-content" href="#collapseBarranquilla">Barranquilla</a></li>
                <li><a data-toggle="collapse" data-parent="#footer-content" href="#collapseBogota">Bogotá</a></li>
                <li><a data-toggle="collapse" data-parent="#footer-content" href="#collapseSantiago">Santiago de Chile</a></li>
              </ul>
              </div>
            
              
              <div class="col-lg-3">
              <div id="city-info">
              <div class="barranquilla collapse in" id="collapseBarranquilla">
                <h4>Barranquilla</h4>
                <span>Calle 77B # 59-61 Oficina 1202</span>
                <span>Centro Empresarial Las Américas II</span>
                <span>Tel.: +576 3694040</span>
                <span>barranquilla@ppulegal.com</span>
              </div>
                <div class="bogota collapse" id="collapseBogota">
                 <h4>Bogotá</h4>
                <span>Carrera 9 # 74-08 Oficina 105</span>
                <span>Tel.: +571 3268600</span>
                <span>bogota@ppulegal.com</span>
              </div>
              

              <div class="santiago collapse" id="collapseSantiago">
                <h4>Santiago de Chile</h4>
                <span>El Golf 40 Piso 20</span>
                <span>Las Condes C.P. 7550107</span>
                <span>Tel: +562 23643700</span>
                <span>Fax: +562 23643796</span>
                <span>santiago@ppulegal.com</span>
              </div>
              </div>
        
              </div>
      
              

              <div class="col-lg-5">
                 <form action="#" class="form-horizontal">
                <div class="form-group">
                  <label for="fields[name]" class="col-sm-2 control-label">Nombre</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="fields[name]">
                  </div>
                </div>
                <div class="form-group">
                  <label for="fields[lastname]" class="col-sm-2 control-label">Apellido</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="fields[lastname]">
                  </div>
                </div>
                <div class="form-group">
                  <label for="fields[email]" class="col-sm-2 control-label">Email</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="fields[email]">
                  </div>
                </div>
                 <div class="form-group">
                  <label for="fields[email]" class="col-sm-2 control-label">Mensaje</label>
                  <div class="col-sm-10">
                    <textarea name="fields[content]" rows="3" class="form-control"></textarea>
                  </div>
                </div>
                 <div class="form-group">
                  <a href="#" class="btn btn-info pull-right"><i class="fa fa-chevron-right"></i></a>
                </div>
              </form>
              </div>
               
              </div>
              <div class="row">
                <div class="col-lg-3">
                  <ul class="social">
                    <li><a href="https://www.linkedin.com/company/1426675?trk=tyah&trkInfo=tarId%3A1420095055704%2Ctas%3Aphilippi%20prie%2Cidx%3A2-1-6" target="_new"><i class="fa fa-linkedin"></i></a></li>
                    <li><a href="https://www.facebook.com/prietocarrizosa.abogados?fref=ts" target="_new"><i class="fa fa-facebook"></i></a></li>
                    <li><a href="https://twitter.com/ppulegal" target="_new"><i class="fa fa-twitter"></i></a></li>
                  </ul>
                </div>
              </div>
            </div>
             <a href="#footer-content" class="open-contact open-contact-footer" data-toggle="collapse"><%= t("contact") %> <i class="fa fa-chevron-up"></i></a>
              <div class="pull-right">
                <a href="#" class="policy">Política de tratamiento de datos</a>
              <span class="copyright">Copyright 2014</span>
              </div>
              
            </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
  

<%= render "lawyers/partials/lawyers" %>
<%= render "partials/select_categories" %>
<%= render "partials/share_modal" %>

