<body id="ppu-app" data-paginate="true">
  <div class="container app-container">

 <%= render("layouts/partials/menu_responsive") %>
 <%= render("layouts/partials/filters_mobile") %>
 
  <div class="row">

  <%= render("layouts/partials/menu") %>

    <div class="top-bar-container">
      <div class="container">
        <div class="row">
          <div class="col-lg-2"></div>
          <div class="col-lg-9  col-md-12 col-sm-12 col-xs-12">
            <div id="top-bar"></div>
          </div>
          <div class="col-lg-1 visible-lg change-lang-container">
            <a href="#" class="change-lang-page"><%= t('page_lang') %></a>
          </div>
        </div>
      </div>
    </div>

    <div class="col-lg-10 col-md-12" id="yield-container">
      <%= yield %>
    </div>

    <div class="footer-container">
      <div class="container">
        <div class="row">
          <div class="col-lg-2"></div>
          <div class="col-lg-10">
            <div id="footer">
              <div  class="collapse" id="footer-content">
                   <a href="#footer-content" class="open-contact" data-toggle="collapse"><%= t("contact") %> <i class="fa fa-chevron-down"></i></a>
              <a href="#footer-content" class="close-footer" data-toggle="collapse"><i class="fa fa-times"></i></a>
          
              <hr>
              <div class="row">
                <div class="col-lg-6">
                  
                  <div id="city-info">
                     <div class="row">
                      <div class="col-lg-3">
                        <h4>Santiago de Chile</h4>
                      </div>
                      <div class="col-lg-8">
                        <span>El Golf 40 Piso 20</span>
                        
                        <span>Las Condes C.P. 7550107</span>
                        
                        <span>Tel: +562 23643700</span>
                        
                        <span>Fax: +562 23643796</span>
                        
                        <span>infoCL@ppulegal.com</span>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-lg-3">
                        <h4>Barranquilla</h4>
                      </div>
                      <div class="col-lg-8">
                        <span>Calle 77B # 59-61 Oficina 1202</span>

                        <span>Centro Empresarial Las Américas II</span>

                        <span>Tel.: +576 3694040</span>

                        <span>infoCO@ppulegal.com</span>
                      </div>
                    </div>
                  
                    <div class="row">
                      <div class="col-lg-3">
                        <h4>Bogotá</h4>
                      </div>
                      <div class="col-lg-8">
                        <span>Carrera 9 # 74-08 Oficina 105</span>
                        
                        <span>Tel.: +571 3268600</span>
                        
                        <span>infoCO@ppulegal.com</span>
                      </div>
                    </div>
                   
                  </div>
                </div>

              <div class="col-lg-6">
                <h5 class="form_thanks hidden"><%= t("form_thanks") %></h5>
                <form action="#" class="form-horizontal">
                  <div class="form-group">
                    <label for="fields[country]" class="col-sm-2 control-label"><%= t("form_footer.country") %></label>

                    <div class="col-sm-10 country-container">
                      <select name="fields[country]" class="form-control">
                        <option value="">Seleccionar País</option>
                        <option value="Colombia">Colombia</option>
                        <option value="Chile">Chile</option>
                        
                      </select>
                    </div>
                  </div>

                <div class="form-group">
                  <label for="fields[name]" class="col-sm-2 control-label"><%= t("form_footer.name") %></label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="fields[name]">
                  </div>
                </div>

                <div class="form-group">
                  <label for="fields[lastname]" class="col-sm-2 control-label"><%= t("form_footer.lastname") %></label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="fields[lastname]">
                  </div>
                </div>

                <div class="form-group">
                  <label for="fields[email]" class="col-sm-2 control-label">Email</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="fields[email]">
                  </div>
                </div>

                 <div class="form-group">
                  <label for="fields[email]" class="col-sm-2 control-label"><%= t("form_footer.message") %></label>
                  <div class="col-sm-10">
                    <textarea name="fields[message]" rows="3" class="form-control"></textarea>
                  </div>
                </div>
                
                 <div class="form-group">
                  <a href="#" class="btn btn-info pull-right contact-save"><i class="fa fa-chevron-right"></i></a>
                </div>
              </form>
              </div>
              </div>
          
            </div>

             <a href="#footer-content" class="open-contact open-contact-footer" data-toggle="collapse"><%= t("contact") %> <i class="fa fa-chevron-up"></i></a>
              <div class="pull-right">
                <a href="#" class="policy">Política de tratamiento de datos</a>
              </div>
              <ul class="social">
                    <li><a href="https://www.linkedin.com/company/1426675?trk=tyah&trkInfo=tarId%3A1420095055704%2Ctas%3Aphilippi%20prie%2Cidx%3A2-1-6" target="new"><i class="fa fa-linkedin"></i></a></li>
                    <li><a href="https://www.facebook.com/prietocarrizosa.abogados?fref=ts" target="new"><i class="fa fa-facebook"></i></a></li>
                    <li><a href="https://twitter.com/ppulegal" target="new"><i class="fa fa-twitter"></i></a></li>
                  </ul>
             
              
            </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
  

<%= render "partials/select_categories" %>
<%= render "partials/share_modal" %>
